{
  "name": "@googlemap-react/core",
  "version": "0.1.6",
  "description": "Easier-to-use Google Map & React integration",
  "repository": "git@github.com:googlemap-react/googlemap-react.git",
  "author": "Gabriel Wu <qqbbnease1004@126.com>",
  "homepage": "https://github.com/googlemap-react/googlemap-react#readme",
  "bugs": {
    "url": "https://github.com/googlemap-react/googlemap-react/issues"
  },
  "license": "MIT",
  "keywords": [
    "google map",
    "googlemap",
    "react",
    "react hooks",
    "react context",
    "marker",
    "infowindow",
    "map",
    "street view",
    "places",
    "search",
    "drawing",
    "visualization",
    "geometry",
    "shapes",
    "overlay"
  ],
  "source": "index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "umd": "dist/index.umd.js",
  "dependencies": {
    "loadjs": "^3.6.1",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "7.3.3",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "8.0.6",
    "@types/googlemaps": "3.37.0",
    "@types/jest": "24.0.15",
    "@types/loadjs": "3.5.0",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@types/uuid": "3.4.5",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react-hooks": "1.6.1",
    "microbundle": "0.11.0",
    "react": "16.8.6",
    "react-docgen-typescript": "1.12.5",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1",
    "react-styleguidist": "9.1.11",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3"
  },
  "scripts": {
    "build": "cp ../../*.md . && rm -rf dist && microbundle --jsx React.createElement --strict && rm -rf .rts2_cache_*",
    "dev": "microbundle --jsx React.createElement --strict --watch --format cjs",
    "prepublish": "yarn build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "pretest": "rm -rf dist && cp tsconfig.json tsconfig.json.bak",
    "test": "react-scripts test",
    "posttest": "rm tsconfig.json && mv tsconfig.json.bak tsconfig.json"
  },
  "eslintConfig": {
    "extends": "react-app",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/__test__helpers__/**"
    ]
  },
  "gitHead": "923d80b549d54ebbb79877167b09892703ff234d",
  "publishConfig": {
    "access": "public"
  }
}
